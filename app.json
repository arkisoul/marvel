{
  "expo": {
    "name": "InvoiceApp",
    "version": "1.0.0",
    "sdkVersion": "27.0.0",
    "icon": "./assets/icon.png",
    "slug": "invoiceapp",
    "ios": {
      "bundleIdentifier": "com.invoiceapp.invoiceapp"
    },
    "android": {
      "package": "com.invoiceapp.invoiceapp"
    },
    "loading": {
      "hideExponentText": true,
      "backgroundImage": "./assets/splashscreen.png"
    },
    "splash": {
      "image": "./assets/splashscreen.png",
      "backgroundColor": "#FEF9B0",
      "resizeMode": "cover"
    },
    "orientation": "portrait",
    "primaryColor": "#cccccc",
    "packagerOpts": {
      "assetExts": [
        "ttf"
      ]
    },
    "androidStatusBarColor": "#334393",
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#334393"
    }
  }
}